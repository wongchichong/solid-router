{
    "compilerOptions": {
        "declaration": true,
        "target": "esnext",
        "newLine": "LF",
        "moduleResolution": "node",
        "strict": true,
        "jsx": "preserve",
        "jsxImportSource": "solid-js",
        "outDir": "./dist",
        "module": "esnext",
        "noImplicitAny": false,
        "paths": {
            "solid-js/*": [
                "../solid-js/packages/solid/*"
            ],
            "solid-js": [
                "../solid-js/packages/solid"
            ],
            "@solidjs/router/*": [
                "../solid-router/dist/*"
            ]
        }
    },
    "include": [
        "./src"
    ],
    "exclude": [
        "node_modules/"
    ]
}